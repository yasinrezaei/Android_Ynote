<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ynote">
    <uses-permission android:name="android.permission.SET_ALARM"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SetEmailAlarm"></activity>
        <activity android:name=".test"></activity>
        <activity android:name=".ma"></activity>
        <activity android:name=".SendEmail"></activity>
        <activity android:name=".CreateEmail"></activity>
        <activity android:name=".EmailActivity"></activity>
        <activity android:name=".FirstActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".CreateNote"></activity>
        <activity android:name=".EditNote"></activity>
        <activity android:name=".setalarm"></activity>
        <receiver android:name=".NotificationPublisher"></receiver>

    </application>

</manifest>